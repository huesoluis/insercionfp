SELECT distinct(a.idcentrofct) as idcentrofct, nombrecentro,centrocodificado from alumnos a,centros c where a.idcentrofct=c.idcentrofct
../../../../datos_junio19/centros//CPC_SALES_LAVIAGA_CASTILLO/datos_alumnos_global.csvselect 'nombre','primer_apellido','segundo apellido','telefono','email','codigo ciclo','denominacion ciclo','provincia','grado' union select nombre,primer_apellido,segundo_apellido,telefono,email,codciclo,denciclo,c.provincia as provincia,ci.grado as grado from alumnos a left join respuestas r on a.idalumnofct=r.idalumnofct, centros c,ciclos ci where ci.idciclofct=a.idciclofct and a.idcentrofct=c.idcentrofct and c.idcentrofct=101  into outfile '/home/fpleaks/tmp/brutos.csv' FIELDS TERMINATED BY ',' ENCLOSED BY '"' LINES TERMINATED BY '
